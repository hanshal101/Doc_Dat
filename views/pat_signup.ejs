<!DOCTYPE html>
<html>
<head>
    <%- include('./partials/header') %>
    <title>Doctor Dashboard</title>
</head>
<body>
    <%- include('./partials/pat_nav') %>
    <form action="/patients/signup" method="post">
  <div>
    <div class="container" style="padding-top: 20px">
        <div class="mb-3">
            <label for="fullName" class="form-label">Full Name</label>
            <input type="text" class="form-control" id="fullName" placeholder="Dr Harish Chandra" name="fullName">
          </div>
          <div class="mb-3">
            <label for="email" class="form-label">Email address</label>
            <input type="email" class="form-control" id="email" placeholder="name@example.com" name="email">
          </div>
        <div class="mb-3">
            <label for="password" class="form-label">Password</label>
            <input type="password" class="form-control" id="password" name="password">
          </div>
          <div class="mb-3">
            <label for="phno" class="form-label">Phone Number</label>
            <input type="number" class="form-control" id="phno" placeholder="9753124680" name="phno">
          </div>
          <div class="mb-3">
            <label for="address" class="form-label">Address</label>
            <input type="text" class="form-control" id="address" name="address">
          </div>
          
        <div>
    <button class="btn btn-primary" type="submit">Submit form</button>
  </div>
    </div>

  
</form>
    <%- include('./partials/script') %>
</body>
</html>
